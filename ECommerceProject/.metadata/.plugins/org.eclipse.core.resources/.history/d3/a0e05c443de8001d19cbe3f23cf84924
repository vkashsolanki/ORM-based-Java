public class FactoryProvider {
	
	private static SessionFactory factory;
	
	
	public static SessionFactory getSessionFactory()
	{
		
		try {
			new Configuration 
			
		} catch (Exception e) {
			e.printStackTrace();
		}
		
		return factory;
		
		
	}

}
